{
    "name": "cxl/common-lib",
    "type": "wordpress-plugin",
    "description": "Common Lib for CXL Projects, provides tools such as Hybrid Core framework etc.",
    "authors": [
        {
            "name": "Leho Kraav",
            "email": "leho@conversionxl.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/conversionxl/phpcs-ruleset"
        },
        {
            "type": "vcs",
            "url": "https://github.com/themehybrid/hybrid-core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/themehybrid/hybrid-support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/themehybrid/hybrid-contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/themehybrid/hybrid-tools"
        }
    ],
    "require": {
        "php" : ">=7.4",
        "justintadlock/hybrid-core": "6.0.x-dev#2a53967 as 5.2.0",
        "justintadlock/hybrid-support": "dev-main#5d07b82",
        "themehybrid/hybrid-contracts": "dev-master#e171ea4 as 1.0.0",
        "themehybrid/hybrid-tools": "dev-master#0154820 as 1.0.0"
    },
    "require-dev": {
        "cxl/phpcs-ruleset": "dev-master",
        "vimeo/psalm": "^4.6.4"
    },
    "autoload": {
        "psr-4": {
            "CXL\\CommonLib\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
